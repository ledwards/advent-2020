from @std/app import start, print, exit
from @std/seq import seq, next
from @std/http import get

on start {
  const url = "https://raw.githubusercontent.com/ledwards/advent-2020/main/data/day01.txt";
  const expenses = get(url).getOr("").trim().split("\n").map(toInt64);

  expenses.each(fn (a: int64) {
    const b = expenses.find(fn (bb: int64): bool = a + bb == 2020);
    if b.isOk() {
      print(a);
      print(b);
      print(a * b.getOr(0));
      emit exit 0;
    }
  });

  emit exit 0;
}